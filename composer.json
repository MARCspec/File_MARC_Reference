{
    "name": "ck/file_marc_reference",
    "type": "library",
    "description": "MARCspec adapter for File_MARC",
    "keywords": [ "MARCspec", "MARC 21" ],
    "authors": [
        {
            "name": "Carsten Klee",
            "homepage": "http://collidoscope.de",
            "role": "Developer"
        }
    ],
    "require": {
        "ck/php-marcspec": "@dev"
    },
    "require-dev": {
        "pear/file_marc": "*",
        "pear/validate_ispn": "*",
        "pear/pear_exception": "1.0.0",
        "phpunit/phpunit": "4.5.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "pear/file_marc": "Actually it's required by this package, but check if it's already installed",
        "pear/validate_ispn": "It's required by File_MARC, but check if it's already installed",
        "pear/pear_exception": "It's required by File_MARC, but check if it's already installed"
    },
    "autoload": {
        "classmap": ["src/"]
    }
}
